#start
		.pos 0
		irmovq stack, %rsp
		call main
		halt

#linked list
		.align 8
ele1:
		.quad 0x00a
		.quad ele2
ele2:
		.quad 0x0b0
		.quad ele3
ele3:
		.quad 0xc00
		.quad 0  

main:
		irmovq ele1, %rdi
		call sum
		ret

sum:
		xorq %rax, %rax
		andq %rdi, %rdi
		jmp		test

loop:
		mrmovq (%rdi), %r10
		addq %r10, %rax
		mrmovq 8(%rdi), %rdi
		andq %rdi, %rdi

test:
		jne		loop
		ret

#stack position		
		.pos 0x200
stack:
